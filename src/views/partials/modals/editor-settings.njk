{# MODAL SETTINGS #}
<div id="modal-settings" class="modal fade" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content border-indie">
            <div class="modal-header modal-indie">
                <h5 id="modal-settings-tittle" class="modal-title"></h5>
            </div>
            <div id="modal-settings-body" class="modal-body"></div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">{{translate('cancel')}}</button>
                <button type="button" class="btn btn-indie btn-submit">{{translate('save_changes')}}</button>
            </div>
        </div>
    </div>
</div>